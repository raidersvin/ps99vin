
local Players = game:GetService("Players")
local player = Players.LocalPlayer

while not player do
	task.wait(1)
	player = Players.LocalPlayer
end

local Players = game:GetService("Players")
local Workspace = game:GetService("Workspace")

local function hideMap()
		for _, obj in ipairs(Workspace:GetDescendants()) do
			if obj:IsA("BasePart") then
				obj.Transparency = 1
				obj.CastShadow = false
			elseif obj:IsA("Decal") or obj:IsA("Texture") then
				obj.Transparency = 1
			end
		end
	end

setfpscap(15)
hideMap()

local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local VirtualInputManager = game:GetService("VirtualInputManager")
local LocalPlayer = Players.LocalPlayer


task.wait(10)

VirtualInputManager:SendKeyEvent(true, Enum.KeyCode.F, false, game)
task.wait(0.3)
VirtualInputManager:SendKeyEvent(false, Enum.KeyCode.F, false, game)
task.wait(0.3)
task.wait(1)
VirtualInputManager:SendKeyEvent(true, Enum.KeyCode.F, false, game)
task.wait(0.3)
VirtualInputManager:SendKeyEvent(false, Enum.KeyCode.F, false, game)
task.wait(0.3)


-- ‚úÖ H√†m nh·∫•n ph√≠m E
function pressKeyE()

VirtualInputManager:SendKeyEvent(true, Enum.KeyCode.E, false, game)
task.wait(0.3)
VirtualInputManager:SendKeyEvent(false, Enum.KeyCode.E, false, game)
task.wait(0.3)

end


-- ‚úÖ D·ªãch chuy·ªÉn + b·∫•m E
local function runSequence(teleportPositions)
	for _, pos in ipairs(teleportPositions) do
		local character = player.Character or player.CharacterAdded:Wait()
		local rootPart = character:WaitForChild("HumanoidRootPart")

		-- Teleport ƒë·∫øn v·ªã tr√≠
		rootPart.CFrame = CFrame.new(pos)

		-- ƒê·ª£i v√† nh·∫•n E
		hideMap()
		task.wait(1.5)
		pressKeyE()
		task.wait(1.5)
	end
end



-- üîç Ki·ªÉm tra ƒë√∫ng PlaceId
if game.PlaceId == 140403681187145 then
    


-- ‚úÖ Danh s√°ch v·ªã tr√≠ d·ªãch chuy·ªÉn
local teleportPositions = {
    Vector3.new(-15484.5, 20, -404.5), 
    Vector3.new(-14856.5, 20, -408.5), 
    Vector3.new(-14261.3, 20, 193), 	
    Vector3.new(-14679, 20, 505.5), 	
}




-- üîÅ Ch·∫°y sau khi game load ho√†n t·∫•t
if RunService:IsRunning() then
    task.wait(2)
    runSequence(teleportPositions)

end

game:GetService("ReplicatedStorage"):WaitForChild("Network"):WaitForChild("World3Teleport"):InvokeServer()
end

